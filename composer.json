    "autoload": {
        "psr-4": {
            "DesignPatterns\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DesignPatterns\\Test\\": "test"
        }
    },
    "config": {
        "optimize-autoloader": true
    }
}
